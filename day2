#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
//sizeof
int main()
{
	int a = 10;
	int arr[10] = { 0 };
	int num = 0;
	num = sizeof(arr) / sizeof(arr[0]);
	printf("%d\n", sizeof(a));//sizeof计算的是变量/类型所占空间的大小，单位是字节
	printf("%d\n", sizeof(arr));
	printf("数组元素个数=%d\n", num);//数组元素个数=数组总大小/每个元素的大小
	return 0;
}
